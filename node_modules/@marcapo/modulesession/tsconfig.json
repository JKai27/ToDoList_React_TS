{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["es2019",
    "es2015",
    "es2016",
    "esnext",
    "esnext.asynciterable"],
    "outDir": "lib",
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "declaration": true,
    "declarationDir": "lib",
    "sourceMap": true,
    "typeRoots": ["./node_modules/@types", "./types/express.d.ts"]
  },
}
